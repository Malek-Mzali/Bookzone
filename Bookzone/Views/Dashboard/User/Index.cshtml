@{
    ViewBag.Title = "Users";
    Layout = "_DashboardLayout";
}

@model Bookzone.Models.Entity.User.UserInfo

<div class="card">
    <div class="card-header">
        <h3 class="card-title">@ViewContext.RouteData.Values["id"] informations</h3>
    </div>
    <!-- /.card-header -->
    <div class="card-body">
        <table id="UsersList" class="table table-hover  nowrap dt-body-center">
            <thead>
            <tr>
                <th>Id</th>
                <th>Photo</th>
                <th>Name</th>
                <th>Email</th>
                <th>Password</th>
                <th>Date</th>
                <th style="width: 5px;">Action</th>
            </tr>
            </thead>

        </table>
    </div>
    <!-- /.card-body -->
</div>


@section Scripts{
    <script src="~/js/datatables.min.js"></script>
    <script src="~/js/Dashboard/User/UsersList.js" asp-append-version="true"></script>
        <script src="~/js/Dashboard/Shared.js" asp-append-version="true"></script>

}


<div class="modal fade" id="editUser" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
<div class="modal-dialog modal-dialog-centered modal-dialog-scrollable">
<div class="modal-content">
<div class="modal-header">
    <h5 class="modal-title" id="exampleModalLabel">@ViewContext.RouteData.Values["id"]</h5>
    <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
</div>
<div class="modal-body">
<form id="edituserform" method="post">
<div class="card-body">
<div class="row">
<input id="typeoflist" type="hidden"  value="@ViewContext.RouteData.Values["id"]">
<input id="txtId" type="hidden" asp-for="UsersGroup.Id">

<div class="row mb-3">
    <div class="col-md-3">
        <div class="avatar avatar-xxl position-relative">
            <img src="" id="txtPhoto" class="border-radius-md">
            <input type="file" data-type="profile" id="PhotoUpload" accept="image/png, image/jpeg"  style="display:none"/>
            <label for="PhotoUpload" class="btn btn-sm btn-icon-only bg-gradient-light position-absolute bottom-0 end-0 mb-n2 me-n2">
                <i class="fa fa-pen top-0" data-bs-toggle="tooltip" data-bs-placement="top" title="" aria-hidden="true" data-bs-original-title="Edit Image" aria-label="Edit Image"></i><span class="sr-only">Edit Image</span>
            </label>
            <button type="button" id="deletePicture" class="btn btn-sm btn-icon-only bg-gradient-light position-absolute bottom-3 end-0 me-n2" data-type="profile">
                <i class="fa fa-remove top-0" data-bs-toggle="tooltip" data-bs-placement="top"   aria-hidden="true" data-bs-original-title="Delete Image" aria-label="Delete Image"></i><span class="sr-only">Edit Image</span>
            </button>
            
        </div>

    </div>

    <div class="col-md-3 mx-2 mt-5">
        <div class="progress" id="PicturePBClass" style="display: none">
            <div id="PicturePB" class="progress-bar"   >
            </div>
        </div>
    </div>
    
</div>




<div class="col-md-3">
    <div class="form-group">
        <label>Email</label>&nbsp;<span style="color:red;">*</span>&nbsp;<span class="text-danger" data-valmsg-replace="true"></span>
